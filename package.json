    "update-schema": "cd infrastructure/hasura && hasura migrate apply && hasura metadata apply",
    "seed": "cd infrastructure/hasura && hasura seed apply",
    "codegen": "cd apps/travel-buddy && graphql-codegen --watch"
  },
